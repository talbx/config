create_commit(){
    echo "running git commit -m '$1: $2'"
    git commit -m "$1: $2"
}

commit_feat(){
    create_commit feat "$1"
}

commit_fix(){
    create_commit fix "$1"
}

commit_chore(){
    create_commit chore "$1"
}

commit_test(){
    create_commit test "$1"
}

commit_cleanup(){
    create_commit cleanup "$1"
}

commit_fixup(){
    create_commit fixup "$1"
}

commit_docs(){
    create_commit docs "$1"
}

squash_last(){
  git reset --soft HEAD~$1
}

eval "$(starship init bash)"
