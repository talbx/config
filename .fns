function create_commit {
    git commit -m "$1: $2"
}

function commit_fix {
    create_commit "fix" "$1"
}

function commit_feat {
    create_commit "feat" "$1"
}

function commit_chore {
    create_commit "chore" "$1"
}

function commit_test {
    create_commit "test" "$1"
}

function commit_refactor {
    create_commit "refactor" "$1"
}